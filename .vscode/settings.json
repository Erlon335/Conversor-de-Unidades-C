{
    "files.associations": {
        "*.embeddedhtml": "html",
        "*.h": "c",
        "*.c": "c"
    },
    "C_Cpp.default.configurationProvider": "ms-vscode.cpptools",
    "C_Cpp.intelliSenseMode": "gcc-x64",
    "C_Cpp.default.compilerPath": "C:/MinGW/bin/gcc.exe",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/include",
        "C:/MinGW/include"
    ]
}
